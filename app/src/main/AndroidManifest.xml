<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.beyondbell.multility">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <!--Management-->
        <activity android:name=".LandingScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainMenu"/>
        <activity android:name=".settings.Settings"/>
        <!--Sensors-->
        <activity android:name=".sensors.Accelerometer"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.AmbientTemperature"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.Gravity"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.Gyroscope"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.Light"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.LinearAcceleration"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.Compass"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.Orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.Pressure"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.Proximity"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.RelativeHumidity"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.RotationVector"
            android:screenOrientation="portrait"/>
        <activity android:name=".sensors.Temperature"
            android:screenOrientation="portrait"/>

    </application>

</manifest>
